  "type_de_generator": [
    {
      "id": 1,
      "name": "حاسبة الدهان",
      "inputs": [
        {
          "name": "area",
          "label": "المساحة",
          "type": "number",
          "unit": "m²"
        },
        {
          "name": "paint_product_id",
          "label": "نوع الدهان",
          "type": "select",
          "options": [1, 2]
        },
        {
          "name": "wall_condition_id",
          "label": "حالة الجدران",
          "type": "select",
          "options": [15, 16]
        }
      ],
      "pricing_formula": "(area * price_unit) + (Math.ceil(area / (paint_product_id === 1 ? 20 : 25)) * (paint_product_id === 1 ? 75 : 60)) + (wall_condition_id === 15 ? area * 15 : wall_condition_id === 16 ? area * 30 : 0)"
    },
    {
      "id": 6,
      "name": "حاسبة السيراميك",
      "inputs": [
        {
          "name": "floor_area",
          "label": "مساحة الأرضية",
          "type": "number",
          "unit": "m²"
        },
        {
          "name": "ceramic_product_id",
          "label": "نوع السيراميك",
          "type": "select",
          "options": [7, 8]
        },
        {
          "name": "floor_preparation",
          "label": "تحضير الأرضية",
          "type": "boolean"
        }
      ],
      "pricing_formula": "(floor_area * price_unit * (ceramic_product_id === 7 ? 1.2 : 1.0)) + (floor_preparation ? floor_area * 20 : 0) + (Math.ceil(floor_area / 5) * 35) + (floor_area * 15)"
    }
  ],
