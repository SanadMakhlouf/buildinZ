    {
      "id": 1,
      "name": "حاسبة الدهان",
      "inputs": [
        {
          "name": "area",
          "label": "المساحة",
          "type": "number",
          "unit": "m²"
        },
        {
          "name": "paint_product_id",
          "label": "نوع الدهان",
          "type": "select",
          "options": [1, 2]
        },
        {
          "name": "wall_condition_id",
          "label": "حالة الجدران",
          "type": "select",
          "options": [15, 16]
        }
      ],
      "pricing_formula": "(area * price_unit) + (Math.ceil(area / (paint_product_id === 1 ? 20 : 25)) * (paint_product_id === 1 ? 75 : 60)) + (wall_condition_id === 15 ? area * 15 : wall_condition_id === 16 ? area * 30 : 0)"
    },
    {
      "id": 2,
      "name": "حاسبة ورق الجدران",
      "inputs": [
        {
          "name": "wall_length",
          "label": "طول الجدار",
          "type": "number",
          "unit": "m"
        },
        {
          "name": "wall_height",
          "label": "ارتفاع الجدار",
          "type": "number",
          "unit": "m"
        },
        {
          "name": "wallpaper_product_id",
          "label": "نوع ورق الجدران",
          "type": "select",
          "options": [3]
        }
      ],
      "pricing_formula": "(wall_length * wall_height) * price_unit + (Math.ceil(wall_length * wall_height / 3) * 45) + ((wall_length * wall_height) * 10)"
    },
    {
      "id": 3,
      "name": "حاسبة دهان الأسقف",
